#!/bin/bash

# usage
cmdname=`basename $0`

function usage() {
  echo "Usage: ${cmdname} subcommand" 1>&2
}

function open_url() {
  open 'https://github.com/Kurupeku/OFFER_CHRONICLE/pulls'
  open 'https://github.com/medfie/pred/pulls'
  open 'https://github.com/imamura0825/step-sales/pulls'
  open 'https://github.com/imamura0825/template-example/pulls'
  open 'https://github.com/mofmof/sms-porter/pulls'
  open 'https://github.com/mofmof/sms-control/pulls'
  open 'https://github.com/mofmof/browser/pulls'
  open 'https://github.com/mofmof/sms-dummy/pulls'
  open 'https://github.com/mofmof/spacenet/pulls'
  open 'https://github.com/SuperCrowds/rili-api/pulls'
  open 'https://github.com/mofmof/donusagi-bot/pulls'
}

if [ $# -ne 1 ]; then
  usage
  exit 1
fi

subcommand="$1"
shift

case $subcommand in
    open)
        open_url
        ;;
    *)
        echo "default"
        ;;
esac

exit 0
